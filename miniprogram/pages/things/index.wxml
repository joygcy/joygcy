<view class="container">
  <view
    wx:for="{{list}}"
    wx:key="{{item._id}}"
    class="item"
  > 
    <view class="header">
      <view class="title">{{item.title}}</view>
      <view class="date">{{item.date}}</view>
    </view>

    <view class="content">
      {{item.description}}
    </view>
  </view>
</view>